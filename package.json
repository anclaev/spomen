{
  "name": "workspace",
  "version": "0.0.0",
  "description": "Spomen workspace",
  "author": "Artem Samarin <iahugo@yandex.ru> (https://t.me/anclaev)",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "commit": "yarn git-cz",
    "push": "git push --follow-tags origin && git push --follow-tags gitverse",
    "lint": "nx run-many -t lint --all --skip-nx-cache",
    "lint:fix": "nx run-many -t lint --all --fix --skip-nx-cache",
    "run-services": "bash scripts/run-services.sh",
    "generate-proto": "bash scripts/generate-proto.sh",
    "merge-schema": "bash scripts/merge-schema.sh",
    "dev": "nx serve",
    "dev:all": "nx run-many -t serve --all --skip-nx-cache",
    "test:all": "nx run-many -t test --all --skip-nx-cache --output-style=stream"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog",
      "disableScopeLowerCase": false,
      "disableSubjectLowerCase": false
    }
  },
  "lint-staged": {
    "*.(ts|tsx|sass|css)": [
      "prettier --write",
      "yarn lint --fix"
    ],
    "*.md": "prettier --write"
  },
